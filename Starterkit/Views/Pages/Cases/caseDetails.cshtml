                                    <div class="row">
                                        <!--begin::Col-->
                                        <div class="col-lg-6 col-md-12 fv-row">
                                            <label class="col-lg-4 col-md-4 col-form-label fw-bold fs-6">Full Name : </label>
                                            <label id="lblOppFullName" runat="server"></label>
                                        </div>
                                        <!--end::Col-->
                                    </div>
                                    <!--end::Row-->
                                </div>
                                <!--end::Input group-->
                                <!--begin::Input group-->
                                <div class="row mb-0 mb-xl-6">
                                    <!--begin::Row-->
                                    <div class="row">
                                        <!--begin::Col-->
                                        <div class="col-lg-6 col-md-12 fv-row">
                                            <label class="col-lg-4 col-md-4 col-form-label fw-bold fs-6">Email : </label>
                                            <label id="lblOppEmail" runat="server"></label>
                                        </div>
                                        <!--end::Col-->
