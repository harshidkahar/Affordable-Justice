                                        <div class="col-lg-6 col-md-12 fv-row">
                                            <label class="col-lg-4 col-md-4 col-form-label fw-bold fs-6">Current Case Number </label>
                                            <label id="lblCurrCaseNumber" runat="server"></label>
                                        </div>
                                        <!--end::Col-->
                                    </div>
                                    <!--end::Row-->
