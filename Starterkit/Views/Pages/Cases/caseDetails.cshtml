                                    <div class="row">
                                        <!--begin::Col-->
                                        <div class="col-lg-6 col-md-12 fv-row">
                                            <label class="col-lg-4 col-md-4 col-form-label fw-bold fs-6">Any legal advice inferred </label>
                                            <label id="lblAnyLegal" runat="server"></label>
                                        </div>
                                        <!--end::Col-->
                                        <!--begin::Col-->
                                        <div class="col-lg-6 col-md-12 fv-row">
                                            <label class="col-lg-4 col-md-4 col-form-label fw-bold fs-6">Date commenced </label>
                                            <label id="lblDateCom" runat="server"></label>
                                        </div>
                                        <!--end::Col-->
                                    </div>
                                    <!--end::Row-->
                                </div>
                                <!--end::Input group-->
                                <!--begin::Input group-->
                                <div class="row mb-0 mb-xl-6">
                                    <!--begin::Row-->
                                    <div class="row">
                                        <!--begin::Col-->
                                        <div class="col-lg-6 col-md-12 fv-row ">
                                            <label class="col-lg-4 col-md-4 col-form-label fw-bold fs-6">Previouse Case Number : </label>
                                            <label id="lblPreCaseNumber" runat="server"></label>
                                        </div>
                                        <!--end::Col-->
