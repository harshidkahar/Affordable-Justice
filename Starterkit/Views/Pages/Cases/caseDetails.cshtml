                                            <label class="col-lg-4 col-md-4 col-form-label fw-bold fs-6">Emirates ID : </label>
                                            <label id="lblOppEmiratesId" runat="server"></label>
                                        </div>
                                        <!--end::Col-->
                                        <!--begin::Col-->
                                        <div class="col-lg-6 col-md-12 fv-row">
                                            <label class="col-lg-4 col-md-4 col-form-label fw-bold fs-6">Passport Number : </label>
                                            <label id="lblOppPass" runat="server"></label>
                                        </div>
                                        <!--end::Col-->

                                    </div>
                                    <!--end::Row-->
                                </div>
                                <!--end::Input group-->
