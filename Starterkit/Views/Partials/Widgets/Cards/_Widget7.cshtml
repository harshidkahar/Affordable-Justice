 <!--begin::Card widget 11-->
<div class="card card-flush h-xl-100" style="background-color: #F6E5CA">
    <!--begin::Header-->
    <div class="card-header flex-nowrap pt-5">
        <!--begin::Title-->
        <h3 class="card-title align-items-start flex-column">
            <span class="card-label fw-bold fs-4 text-gray-800">Cases</span>
            <span class="mt-1 fw-semibold fs-7" id="lblTotalCases" runat="server">Total Case : 10</span>
        </h3>
        <!--end::Title-->
    </div>
    <!--end::Header-->
    <!--begin::Body-->
    <div class="card-body text-center pt-5">
        <!--begin::Image-->
        <img src="assets/media/logos/logo_icon.png" class="h-125px mb-5" alt="" />
        <!--end::Image-->
        <!--begin::Section-->
        <div class="text-start">
            <h4 class="text-dark">
                Assigned Cases :
                <span class="mt-1 fw-semibold" id="lblActiveCase" runat="server">10</span>
            </h4>
            <h4 class="text-dark">
                Inactive Cases :
                <span class="mt-1 fw-semibold" id="lblInActiveCases" runat="server">10</span>
            </h4>
            <h4 class="text-dark">
                In Process Cases :
                <span class="mt-1 fw-semibold" id="lblInProcessCases" runat="server">10</span>
            </h4>
        </div>
        <!--end::Section-->
    </div>
    <!--end::Body-->
</div>
<!--end::Card widget 11-->