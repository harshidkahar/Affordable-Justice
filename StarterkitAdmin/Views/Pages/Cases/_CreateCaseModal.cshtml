                            <label class="required fs-6 fw-semibold mb-2">Case Type</label>
                            <select class="form-select form-select-solid" data-control="select2" data-placeholder="Open" data-hide-search="true">
                                <option value="1" selected="selected">COMPLAINANT</option>
                                <option value="2">RESPONDENT</option>
                            </select>
                            <!--end::Label-->
                        </div>
                        <!--end::Col-->
                        <!--begin::Col-->
                        <div class="col-md-6 fv-row">
                            <!--begin::Label-->
                            <label class="required fs-6 fw-semibold mb-2">Case Type</label>
                            <select class="form-select form-select-solid" data-control="select2" data-placeholder="Open" data-hide-search="true" id="caseType1">
                                <option value="CIVIL" selected="selected">CIVIL</option>
                                <option value="CRIMINAL">CRIMINAL</option>
                                <option value="OTHERS">OTHERS</option>
                                <option value="PERSONAL STATUS">PERSONAL STATUS</option>
                            </select>
